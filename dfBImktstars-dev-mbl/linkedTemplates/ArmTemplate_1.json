						"          DataInicio as date,",
						"          DataFim as date",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> Stage",
						"Source sort(asc(Id, true)) ~> ClassSource",
						"ClassSource select(mapColumn(",
						"          Id_SRC = Id,",
						"          Nome_SRC = Nome",
