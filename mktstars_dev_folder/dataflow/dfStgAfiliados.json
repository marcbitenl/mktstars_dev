{
	"name": "dfStgAfiliados",
	"properties": {
		"folder": {
			"name": "2 - Stage"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "tblsrcAfiliados",
						"type": "DatasetReference"
					},
					"name": "Source"
				},
				{
					"dataset": {
						"referenceName": "tblStgAfiliados",
						"type": "DatasetReference"
					},
					"name": "Stage"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ClassSource"
				},
				{
					"name": "NewSource"
				},
				{
					"name": "ClassStage"
				},
				{
					"name": "NewStage"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as integer,",
				"          Nome as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Source",
				"source(output(",
				"          Id as integer,",
				"          BK as integer,",
				"          Nome as string,",
				"          DataInicio as date,",
				"          DataFim as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Stage",
				"Source sort(asc(Id, true)) ~> ClassSource",
				"ClassSource select(mapColumn(",
				"          Id_SRC = Id,",
				"          Nome_SRC = Nome",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> NewSource",
				"Stage sort(asc(BK, true)) ~> ClassStage",
				"ClassStage select(mapColumn(",
				"          Id_STG = Id,",
				"          BK_STG = BK,",
				"          Nome_STG = Nome,",
				"          DataInicio_STG = DataInicio,",
				"          DataFim_STG = DataFim",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> NewStage"
			]
		}
	}
}